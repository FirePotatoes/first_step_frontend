<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".goalsetting"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-19dp">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:text="+ 목표 추가"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*목표 설정 후 30분 이내에 입금하시지 않으면 목표설정이 취소 됩니다."
        android:textColor="#FF0057"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_goalsettingcomplete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_goalsettingcomplete"
        android:layout_width="325dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="완료"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText3" />


    <Button
        android:id="@+id/button5"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:layout_marginStart="170dp"
        android:layout_marginBottom="8dp"
        app:icon="?attr/actionModeCopyDrawable"
        app:iconSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="325dp"
        android:layout_height="55dp"
        android:text="입금계좌: 기업은행\n1111223333444 버킷랩"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <EditText
        android:id="@+id/editText"
        android:layout_width="160dp"
        android:layout_height="49dp"
        android:layout_marginHorizontal="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="169dp"
        android:layout_marginBottom="33dp"
        android:hint="달력으로 날짜를 선택해주세요"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/categoryComboBox"
        app:layout_constraintEnd_toStartOf="@+id/btn_goalsettingcalendar"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="242dp"
        android:layout_height="39dp"
        android:layout_marginHorizontal="60dp"
        android:layout_marginBottom="64dp"
        android:hint=""
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="190dp"
        android:layout_height="0dp"
        android:layout_marginHorizontal="60dp"
        android:layout_marginBottom="272dp"
        android:hint=""
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_goalsettingcomplete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/categoryComboBox"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="23dp"
        android:text="하루목표시간"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/categoryComboBox"
        app:layout_constraintEnd_toStartOf="@+id/categoryComboBox"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/categoryComboBox" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="90dp"
        android:layout_height="37dp"
        android:layout_marginBottom="12dp"
        android:text="예치금"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/editText3"
        app:layout_constraintStart_toStartOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="81dp"
        android:layout_height="46dp"
        android:layout_marginBottom="200dp"
        android:text="목표 기간"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/textView6" />

    <Spinner
        android:id="@+id/categoryComboBox2"
        android:layout_width="190dp"
        android:layout_height="51dp"
        android:layout_marginTop="28dp"
        android:layout_weight="1"
        android:spinnerMode="dropdown"
        android:text="-선택해주세요"
        app:layout_constraintEnd_toEndOf="@+id/editText3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText3"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/categoryComboBox"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="32dp"
        android:layout_weight="1"
        android:spinnerMode="dropdown"
        android:text="-선택해주세요"
        app:layout_constraintBottom_toTopOf="@+id/editText3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="90dp"
        android:layout_height="37dp"
        android:text="환불계좌"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/categoryComboBox2"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/categoryComboBox2"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/btn_goalsettingcalendar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="516dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/categoryComboBox"
        app:srcCompat="@android:drawable/ic_menu_my_calendar"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>