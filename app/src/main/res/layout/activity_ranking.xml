<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ranking">

    <View
        android:id="@+id/dividerLeft"
        android:layout_width="0.1dp"
        android:layout_height="350dp"
        android:layout_gravity="center"
        android:layout_marginTop="180dp"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toStartOf="@+id/tv_grade5"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/dividerRight"
        android:layout_width="0.1dp"
        android:layout_height="350dp"
        android:layout_gravity="center"
        android:layout_marginTop="180dp"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintStart_toEndOf="@+id/divider4"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_grade5"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 5위"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade1"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade4"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/tv_grade6"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 6위"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade1"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade5"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/tv_grade7"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 7위"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade6"
        app:layout_constraintStart_toStartOf="@+id/tv_grade6"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade6"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/tv_grade8"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/toomyeong"
        android:gravity="center_vertical"
        android:text=" 나"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomBar"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade6"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/rankingTitleView"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="32dp"
        android:text="+ 랭킹"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/settingIcon"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="40dp"
        android:rotation="-19"
        android:rotationX="28"
        android:rotationY="28"
        android:src="@drawable/icon_star"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/settingIcon3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="120dp"
        android:rotation="-208"
        android:rotationX="16"
        android:rotationY="22"
        android:src="@drawable/icon_star"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintBottom_toBottomOf="@+id/tv_grade2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade2"
        tools:ignore="MissingConstraints" />
    <View
        android:id="@+id/divider6"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintBottom_toBottomOf="@+id/tv_grade6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_grade1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="180dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 1위"
        android:gravity="center_vertical"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlCompat" />

    <View
        android:id="@+id/divider1"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_grade2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 2위"
        android:textAlignment="textStart"
        android:gravity="center_vertical"
        android:textColor="@color/purple"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade1"
        app:layout_constraintStart_toStartOf="@+id/tv_grade1"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade1" />




    <View
        android:id="@+id/divider3"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_grade3"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 3위"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:gravity="center_vertical"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_grade1"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade2" />

    <TextView
        android:id="@+id/tv_grade4"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/toomyeong"
        android:text=" 4위"
        android:textAlignment="textStart"
        android:textColor="@color/purple"
        android:textSize="20dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade1"
        app:layout_constraintStart_toStartOf="@+id/tv_grade1"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade3" />

    <View
        android:id="@+id/divider5"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade5"
        app:layout_constraintStart_toStartOf="@+id/tv_grade5"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade5"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/divider4"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade4"
        app:layout_constraintStart_toStartOf="@+id/tv_grade4"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade4"/>

    <View
        android:id="@+id/divider7"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade7"
        app:layout_constraintStart_toStartOf="@+id/tv_grade7"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade7"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/divider8"
        android:layout_width="200dp"
        android:layout_height="0.1dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="@+id/tv_grade7"
        app:layout_constraintStart_toStartOf="@+id/tv_grade7"
        app:layout_constraintTop_toBottomOf="@+id/tv_grade8"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/bottomBar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@drawable/bottomboxborder"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_calendar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:backgroundTint="@color/toomyeong"
        android:gravity="center_horizontal"
        android:text="캘린더"
        android:textColor="@color/purple"
        android:textStyle="bold"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/bottomBar"
        app:layout_constraintTop_toTopOf="@+id/bottomBar" />

    <Button
        android:id="@+id/btn_timer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:backgroundTint="@color/toomyeong"
        android:gravity="center_vertical"
        android:textStyle="bold"
        android:text="타이머"
        android:textColor="@color/purple"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/bottomBar"
        app:layout_constraintStart_toEndOf="@+id/btn_calendar"
        app:layout_constraintTop_toTopOf="@+id/bottomBar"
        app:layout_constraintVertical_bias="0.555" />

    <Button
        android:id="@+id/btn_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:backgroundTint="@color/toomyeong"
        android:gravity="center_vertical"
        android:text="설정"
        android:textColor="@color/purple"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_timer"
        app:layout_constraintTop_toTopOf="@+id/bottomBar" />

</androidx.constraintlayout.widget.ConstraintLayout>
